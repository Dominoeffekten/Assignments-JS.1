'use strict';

console.log('Conjunction of 3');
console.log('var1\tvar2\tvar3');

var var1 = true; 
var var2 = true;
var var3 = true;
console.log(var1 + '\t' + var2 + '\t' + var3);

var var1 = true;
var var2 = true;
var var3 = false;
console.log(var1 + '\t' + var2 + '\t' + var3);

var var1 = true;
var var2 = false;
var var3 = false;
console.log(var1 + '\t' + var2 + '\t' + var3);

var var1 = false;
var var2 = false;
var var3 = false;
console.log(var1 + '\t' + var2 + '\t' + var3);

var var1 = false;
var var2 = true;
var var3 = false;
console.log(var1 + '\t' + var2 + '\t' + var3);

var var1 = false
var var2 = true;
var var3 = true;
console.log(var1 + '\t' + var2 + '\t' + var3);

var var1 = false;
var var2 = true;
var var3 = false;
console.log(var1 + '\t' + var2 + '\t' + var3);

